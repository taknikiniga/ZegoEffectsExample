                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="自然"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyelinerdir/eyelinerdir_natural.bundle/"/>

                <RadioButton
                    android:id="@+id/eyeliner_wild_cat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="野猫"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyelinerdir/eyelinerdir_wildcat.bundle/"/>

                <RadioButton
                    android:id="@+id/eyeliner_naughty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="俏皮"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyelinerdir/eyelinerdir_pretty.bundle/"/>

                <RadioButton
                    android:id="@+id/eyeliner_scheming"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="心机"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyelinerdir/eyelinerdir_scheming.bundle/"/>

                <RadioButton
                    android:id="@+id/eyeliner_classy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="气质"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyelinerdir/eyelinerdir_temperament.bundle/"/>
            </RadioGroup>

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/eyeshadow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="80"
                app:maxStr="100"
                app:minStr="0"
                app:title="眼影: "
                app:switchVisibility="false"
                />

            <RadioGroup
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/eyeshadow_list">

                <RadioButton
                    android:id="@+id/eyeshadow_original"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="原图"
                    android:checked="true"
                    android:tag=""/>

                <RadioButton
                    android:id="@+id/eyeshadow_misty_pink"
                    android:layout_width="wrap_content"
