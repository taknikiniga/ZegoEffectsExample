                <RadioButton
                    android:id="@+id/eyeshadow_vital_orange"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="元气橙"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyeshadowdir/eyeshadowdir_vitality_orange.bundle/"/>
                <RadioButton
                    android:id="@+id/eyeshadow_brown"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="摩卡棕"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/eyeshadowdir/eyeshadowdir_mocha_brown.bundle/"/>
