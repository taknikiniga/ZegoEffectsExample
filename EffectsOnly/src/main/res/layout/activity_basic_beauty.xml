                <RadioButton
                    android:id="@+id/eyecolor_eyes_color_starry_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="星空蓝"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/coloredcontactsdir/coloredcontactsdir_starry_blue.bundle/"/>

                <RadioButton
                    android:id="@+id/eyecolor_eyes_color_fairy_brown_green"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="仙棕绿"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/coloredcontactsdir/coloredcontactsdir_mystery_brown_green.bundle/"/>

                <RadioButton
                    android:id="@+id/eyecolor_eyes_color_different_pupil_brown"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="异瞳棕"
                    android:checked="false"
                    android:tag="Resources/MakeupResources/coloredcontactsdir/coloredcontactsdir_polar_lights_brown.bundle/"/>
                <RadioButton
                    android:id="@+id/eyecolor_eyes_color_cocoa_brown"
