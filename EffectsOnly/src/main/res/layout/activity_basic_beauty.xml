        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:paddingHorizontal="32dp"
        android:alpha="0.7"
        android:background="@color/white"
        android:scrollbars="none"
        android:visibility="visible">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingVertical="16dp">


            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/smooth"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="磨皮: " />

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/whiten_skin"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="美白: " />


            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/rosy"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="红润: " />

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/sharpen"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="锐化: " />

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/wrinkles_removing"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="去法令纹: " />

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
                android:id="@+id/darkCircles_removing"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                app:currentStr="50"
                app:maxStr="100"
                app:minStr="0"
                app:title="去黑眼圈: " />

            <im.zego.effectsexample.effectsonly.widgets.CustomMinSeekBar2
